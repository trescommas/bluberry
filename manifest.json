{
  "manifest_version": 2,

  "name": "Blueberry",
  "description": "alpha-v3",
  "version": "0.3",

	"icons": { "16": "16.png",
           "48": "48.png",
          "128": "128.png" },
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "index.html"

  },
	"chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "permissions": [
    "activeTab",
    "tabs"
  ]

}
